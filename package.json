  "version": "1.1.1",
  "private": true,
  "author": "AlgoWorld <info@algoworld.io>",
  "license": "GPL-3.0",
  "keywords": [
    "nextjs",
    "algoworld",
    "algorand"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "start": "next build && next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "test": "jest --watch",
    "test:ci": "jest --ci --collectCoverage",
    "test:e2e:ci": "playwright test --workers=1",
    "test:e2e:ci:local": "E2E_TESTS_BASE_URL=http://localhost:3000 playwright test --workers=1",
    "test:e2e:local": "E2E_TESTS_BASE_URL=http://localhost:3000 playwright test --workers=1 --headed",
    "commit": "cz",
    "pre-commit": "yarn format && yarn lint && yarn type-check && yarn build"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
